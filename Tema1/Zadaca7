#include <stdio.h>
#include <stdlib.h>
//Напиши програма каде што преку прееку тастатура ќе може да се внесат два броеви(b,h).
//Пронајдиа плоштината(A) според наведената формула и отпечати го резултатот на екран.


int main()
{
    float b,h;

    scanf("%f", &b);
    scanf("%f", &h);

    float A = b*h;

    printf("The result is: %f", A);


    return 0;
}
