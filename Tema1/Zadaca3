#include <stdio.h>
#include <stdlib.h>

//Напиши програма каде што преку тастатура ќе може да се внеси цел број.
//Провери дали бројот е непарен и е делив со 17.
//Резултатот отпечатиго на екран.

int main()
{
    int number;

    scanf("%d",&number);

    int result = number % 17 == 0;

    // 1 for even number
    // 0 for odd number
    int isEven = result % 2 == 0;

    printf("The number is even: %d", isEven);

    return 0;
}
