#include <stdio.h>
#include <math.h>

//Напиши програма каде што преку тастатура ќе може да се внесат три броеви(a,b,x)
//Да се пресмета изразот (y) и да се отпечати резултатот.
int main()
{
    float a,b,x;

    scanf("%f", &a);
    scanf("%f", &b);
    scanf("%f", &x);

    double result= (pow(a,3)+(2/(x+b)))/sqrt((5*a)+pow(b,2));

    printf("The result is: %f", result);

    return 0;
}
