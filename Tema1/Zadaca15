#include <stdio.h>
#include <math.h>

//Напиши програма каде што преку тастатура ќе може да се внесе еден број(x)
//Пронајдиго резултатот(y)и отпечатиго на екран.
int main()
{
    float x;

    scanf("%f", &x);

    float y= 11*pow(x,3)/5 + 12*pow(x,7) -(3.5);

    printf("The result is: %f", y);

    return 0;
}
