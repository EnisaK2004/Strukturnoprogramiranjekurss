#include <stdio.h>
#include <math.h>
//Напиши програма каде што преку тастатура ќе може да се внесат три броеви(a,b)
//Да се пресмета изразот (y) и да се отпечати резултатот.

int main()
{
    float a,b;

    scanf("%f", &a);
    scanf("%f", &b);

    double result= sqrt((pow(a,2)+3)/(1+(pow(b,3)/a)))/((a/(pow(b,4)))+sqrt((1/(a+b))));

    printf("The result is: %f", result);

    return 0;
}
