#include <stdio.h>
#include <stdlib.h>
//Напиши програма каде што преку тастатура ќе може да се внесат три броеви(L,w,h)
//пронајдиго волуменот според формулата v =Lwh.Отпешчатиго резултатот на екран.
int main()
{
    float L,w,h;

    scanf("%f", &L);
    scanf("%f", &w);
    scanf("%f", &h);

    float v =L*w*h;

    printf("The result is: %f", v);

    return 0;
}
