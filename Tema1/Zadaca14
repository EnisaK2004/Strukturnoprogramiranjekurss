#include <stdio.h>
#include <math.h>

//Напиши програма каде што преку тастатура ќе може да се внесат два броеви(a,b)
//Пронајдиго резултатот(y) и отпечатиго на екран.

int main()
{
  float a,b;

  scanf("%f", &a);
  scanf("%f", &b);

  float y= pow(a,3)/5 + 5*pow(a,2)/b -12*b/pow(a,4) + (2,1);

  printf("The result is: %f", y);


    return 0;
}
