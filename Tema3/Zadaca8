#include <stdio.h>
#include <math.h>

//Напиши програма каде што може да се внесат два цели броеви.
//Ако двете броеви се позитивни да се пресмета квадрат од нивната сума,
//во спротивно да се пресмета сума од нивните квадрати.
int main()
{
    int number1,number2;

    scanf("%d", &number1);
    scanf("%d", &number2);

    double result;

    if(number1 > 0 && number2 > 0)
    {
        result =pow(number1+number2,2);
    }
    else
    {
        result =pow(number1,2) + pow(number2,2);
    }
    printf("The result is %.2f",result);

    return 0;
}
