#include <stdio.h>
#include <stdlib.h>

//Напиши програма каде што преку тастатура ќе може да внесиш еден цел број.
//Програмата треба да провери дали бројот се наоѓа во интервал од 30 до 50
//и цоодветно треба да се отпечати "Бројот %d припаѓа на интервалоот (30,50)".
int main()
{
    int number;

    scanf("%d",&number);

    if(number>= 30 && number<=50)
    {

        printf("The number %d - belongs to interval(30,50)",number);
    }

    return 0;
}
