#include <stdio.h>
#include <stdlib.h>

//Напиши програма каде што може да се внесат три цели броеви.
//Да се отпечати на екран поголемиот од броевите,MAX number.
int main()
{
    int number1,number2,number3;

    scanf("%d", &number1);
    scanf("%d", &number2);
    scanf("%d", &number3);

    int max;

    if(number1>number2 && number1>number3)
    {
        max=number1;
    }
    else if(number2>number1 && number2>number3)
    {
        max=number2;
    }
    else
    {
        max=number3;
    }
    int result = max;

    printf("The result is %d",result);

    return 0;
}
