#include <stdio.h>
#include <math.h>

//Напиши програма каде што може да се внесат два цели броеви.
//Ако двете броеви се деливи со 3  да се пресмета корен од нивниот производ,
//во спротивно да се пресмета сума од нивните корени.

int main()
{
    int number1,number2;

    scanf("%d", &number1);
    scanf("%d", &number2);

    double result;

    if(number1 %3==0 && number2 %3==0)
    {
        result=sqrt(number1*number2);
    }
    else
    {
        result=sqrt(number1) + sqrt(number2);
    }
    printf("The result is %.2f",result);

    return 0;
}
