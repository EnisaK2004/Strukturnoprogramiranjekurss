#include <stdio.h>
#include <stdlib.h>
//Напиши програма каде што преку тастатура ќе овозможува да се внесат три променливи x,y,z.
//Искористи еден израз за да доделиш нова вредност во променливата result така што доколку z
//е помал или еднаков на 25 резултатот ќе биде производот на x и y
//Во спротивно резултатот ќе биде количникот на  x и y.
//Прикажи го резултатот на екран.

int main()
{
    int x,y,z;

    scanf("%d", &x);
    scanf("%d", &y);
    scanf("%d", &z);

    int result;

    (z<=25)? (result=(x*y)):(result=(x/y));

    printf("The result is: %d", result);

    return 0;
}
